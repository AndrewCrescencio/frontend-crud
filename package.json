{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.14.2+sha512.6e2baf77d06b9362294152c851c4f278ede37ab1eba3a55fda317a4a17b209f4dbb973fb250a77abc463a341fcb1f17f17cfa24091c4eb319cda0d9b84278387",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --open",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && npx simple-git-hooks",
    "typecheck": "vue-tsc --noEmit",
    "typecheck:nuxi": "npx nuxi typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "json-server": "json-server --watch ./data/db.json --port 5433",
    "cy:test": "cypress open"
  },
  "dependencies": {
    "@formkit/nuxt": "^1.6.9",
    "@nuxtjs/google-fonts": "3.2.0",
    "@pinia/nuxt": "^0.8.0",
    "@vee-validate/nuxt": "^4.14.7",
    "@vee-validate/yup": "^4.14.7",
    "nuxt": "^3.14.1592",
    "nuxt-security": "^2.1.5",
    "pinia": "^2.2.8",
    "vue": "latest",
    "vue-router": "latest",
    "vue-the-mask": "^0.11.1",
    "yup": "^1.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@cypress/code-coverage": "^3.13.8",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@nuxt/eslint": "^0.7.2",
    "@types/cypress__code-coverage": "^3.10.2",
    "@types/vue-the-mask": "^0.11.5",
    "@unocss/eslint-config": "^0.65.0",
    "add": "^2.0.6",
    "cypress": "^13.16.1",
    "eslint": "^9.16.0",
    "eslint-plugin-format": "^0.1.3",
    "json-server": "0.17.4",
    "lint-staged": "^15.2.10",
    "simple-git-hooks": "^2.11.1",
    "std-env": "^3.8.0",
    "typescript": "^5.7.2",
    "vite-plugin-istanbul": "^6.0.2",
    "vue-tsc": "^2.1.10"
  },
  "pnpm": {
    "patchedDependencies": {
      "vue-tsc@2.1.10": "patches/vue-tsc@2.1.10.patch"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx",
      ".vue"
    ]
  }
}
